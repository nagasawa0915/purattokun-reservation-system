        "timingConfig": {
          "delay": 0,
          "duration": 1200,
          "loop": false
        }
      },
      "expectedOutput": {
        "sequenceType": "transition",
        "transition": {
          "fromAnimation": "yarare",
          "toAnimation": "taiki",
          "transitionType": "damage"
        }
      }
    },
    {
      "name": "チェーンアニメーション",
      "input": {
        "characterId": "test_004",
        "sequenceType": "chain",
        "chain": [
          { "animationName": "syutugen", "duration": 2000 },
          { "animationName": "click", "duration": 1000 },
          { "animationName": "taiki", "duration": 3000, "loop": true }
        ]
      },
      "expectedOutput": {
        "sequenceType": "chain",
        "chain": [
          { "animationName": "syutugen", "duration": 2000, "loop": false },
          { "animationName": "click", "duration": 1000, "loop": false },
          { "animationName": "taiki", "duration": 3000, "loop": true }
        ]
      }
    },
    {
      "name": "フェードイン付きアニメーション",
      "input": {
        "characterId": "test_005",
        "animationName": "taiki",
        "sequenceType": "single",
        "timingConfig": {
          "delay": 0,
          "duration": 3000,
          "loop": true
        },
        "fadeIn": true,
        "fadeDelay": 1000,
        "fadeDuration": 2000
      },
      "expectedOutput": {
        "animationName": "taiki",
        "fadeIn": true,
        "fadeDelay": 1000,
        "fadeDuration": 2000
      }
    }
  ],
