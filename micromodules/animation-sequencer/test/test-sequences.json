{
  "testSequences": [
    {
      "name": "基本待機アニメーション",
      "input": {
        "characterId": "test_001",
        "animationName": "taiki",
        "sequenceType": "single",
        "timingConfig": {
          "delay": 0,
          "duration": 3000,
          "loop": true
        }
      },
      "expectedOutput": {
        "animationName": "taiki",
        "sequenceType": "single",
        "timing": {
          "loop": true,
          "duration": 3000
        }
      }
    },
    {
      "name": "出現→待機トランジション",
      "input": {
        "characterId": "test_002",
        "animationName": "syutugen",
        "sequenceType": "transition",
        "timingConfig": {
          "delay": 1500,
          "duration": 2000,
          "loop": false
        }
      },
      "expectedOutput": {
        "sequenceType": "transition",
        "transition": {
          "fromAnimation": "syutugen",
          "toAnimation": "taiki",
          "transitionType": "appearance"
        }
      }
    },
    {
      "name": "ダメージ→待機トランジション",
      "input": {
        "characterId": "test_003",
        "animationName": "yarare",
