  "boundaryTestCases": [
    {
      "name": "境界内の有効な位置",
      "position": { "x": 600, "y": 400 },
      "boundaries": { "minX": 0, "maxX": 1200, "minY": 0, "maxY": 800 },
      "expectedValid": true,
      "expectedClamped": { "x": 600, "y": 400 }
    },
    {
      "name": "X軸最大値超過",
      "position": { "x": 1300, "y": 400 },
      "boundaries": { "minX": 0, "maxX": 1200, "minY": 0, "maxY": 800 },
      "expectedValid": false,
      "expectedClamped": { "x": 1200, "y": 400 }
    },
    {
      "name": "Y軸最小値未満",
      "position": { "x": 600, "y": -50 },
      "boundaries": { "minX": 0, "maxX": 1200, "minY": 0, "maxY": 800 },
      "expectedValid": false,
      "expectedClamped": { "x": 600, "y": 0 }
    },
    {
      "name": "両軸とも境界外",
      "position": { "x": -100, "y": 900 },
      "boundaries": { "minX": 0, "maxX": 1200, "minY": 0, "maxY": 800 },
      "expectedValid": false,
      "expectedClamped": { "x": 0, "y": 800 }
    }
  ],
  "sensitivityTestCases": [
