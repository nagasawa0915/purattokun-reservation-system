{
  "dragInteractions": [
    {
      "name": "基本ドラッグ操作",
      "input": {
        "interactionType": "drag",
        "targetId": "test_drag_001",
        "eventData": {
          "startPosition": { "x": 100, "y": 100 },
          "clientX": 150,
          "clientY": 120,
          "deltaX": 50,
          "deltaY": 20
        },
        "config": {
          "sensitivity": 1.0,
          "boundaries": { "minX": 0, "maxX": 1200, "minY": 0, "maxY": 800 }
        }
      },
      "expectedOutput": {
        "type": "drag",
        "result": {
          "deltaX": 50,
          "deltaY": 20,
          "newPosition": { "x": 150, "y": 120 },
          "isValid": true
        }
      }
    },
    {
      "name": "境界制限付きドラッグ",
      "input": {
        "interactionType": "drag",
        "targetId": "test_drag_002",
        "eventData": {
          "startPosition": { "x": 1150, "y": 750 },
