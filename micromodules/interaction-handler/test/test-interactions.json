          "clientY": 900,
          "deltaX": 150,
          "deltaY": 150
        },
        "config": {
          "sensitivity": 1.0,
          "boundaries": { "minX": 0, "maxX": 1200, "minY": 0, "maxY": 800 }
        }
      },
      "expectedOutput": {
        "result": {
          "newPosition": { "x": 1200, "y": 800 },
          "isValid": false
        }
      }
    },
    {
      "name": "高感度ドラッグ",
      "input": {
        "interactionType": "drag",
